{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/tmp": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/ios": true,
    "**/android": true,
    "**/dist": true,
    "**/build": true,
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
    "**/bower_components/**": true,
    "**/os/**": true,
    "**/android/**": true,
    "**/dist/**": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#140907",
    "titleBar.activeBackground": "#140907",
    "titleBar.activeForeground": "#FBFAFC"
  } 
}