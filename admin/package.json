{
  "name": "onka-react-admin-core",
  "version": "0.1.3",
  "private": false,
  "license": "SEE LICENSE IN <../LICENCE> file",
  "homepage": "https://github.com/onka13/onka-react-libs",
  "author": {
    "name": "onka",
    "url": "https://github.com/onka13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build:esm && node ./scripts/copyTS.js && yarn wp",
    "build0": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:lib": "yarn build:babel && yarn build:types && node ./scripts/copyTS.js",
    "build:babel": "concurrently \"yarn build:babel:esm && yarn build:babel:umd\" \"yarn build:babel:cjs\"",
    "build:babel:cjs": "SET BABEL_ENV=cjs && babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/js --presets=@babel/env",
    "build:babel:esm": "SET BABEL_ENV=esm && babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/esm",
    "build:babel:umd": "SET BABEL_ENV=umd && babel --source-maps --extensions \".js\" dist/esm --out-dir dist/umd --plugins=transform-es2015-modules-umd",
    "build:types": "tsc -p tsconfig.gen-dts.json",
    "develop": "yarn build:types && yarn build:babel:esm --skip-initial-build --watch --verbose",
    "eslint": "eslint src/*",
    "wp": "webpack --config=webpack.config.js",
    "lint": "eslint src/* --quiet",
    "build:esm": "tsc -m esnext -outDir dist/esm",
    "build:cjs": "tsc -m commonjs --outDir dist/cjs"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^35.4.0",
    "@ckeditor/ckeditor5-react": "^5.0.6",
    "axios": "^1.2.2",
    "lodash": "^4.17.21",
    "react-loadable": "^5.5.0",
    "react-scripts": "5.0.1",
    "tss-react": "^4.5.2",
    "typescript": "~4.9.4",
    "zipcelx": "^1.6.2"
  },
  "devDependencies": {    
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-loadable": "^5.5.6",
    "@types/styled-components": "^5.1.26",
    "@types/zipcelx": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^8.0.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.6.2",
    "react-router-dom": "^6.6.2"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react-hooks/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "dist/main.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts"
}
